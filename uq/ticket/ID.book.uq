
IDX Book (
    id,
    value Value,
);

ENUM ProjectType (
    Storage = 201,
);

ID Project (
    id,
    KEY type ENUM ProjectType,
    KEY item ID,
);

ID History (
    id MINUTE,
    project ID,         -- 科目
    value Value,
    ref ID,             -- 相关凭证
    INDEX project_id(project, id) UNIQUE,
);

IDX Done (
    id,
    done Quantity,
);
